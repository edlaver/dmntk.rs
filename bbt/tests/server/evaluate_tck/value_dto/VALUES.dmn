<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<definitions namespace="https://dmntk.io"
             name="ValueDto"
             xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/">
    <description>Test for ValueDto</description>

    <decision name="TestString">
        <variable typeRef="string" name="TestString"/>
        <informationRequirement>
            <requiredInput href="#_input_string"/>
        </informationRequirement>
        <literalExpression>
            <text>"Hello" + Full Name</text>
        </literalExpression>
    </decision>

    <decision name="TestNumber">
        <variable typeRef="number" name="TestNumber"/>
        <informationRequirement>
            <requiredInput href="#_input_number"/>
        </informationRequirement>
        <literalExpression>
            <text>Age + 2</text>
        </literalExpression>
    </decision>

    <decision name="TestBoolean">
        <variable typeRef="boolean" name="TestBoolean"/>
        <informationRequirement>
            <requiredInput href="#_input_boolean"/>
        </informationRequirement>
        <literalExpression>
            <text>not(Married)</text>
        </literalExpression>
    </decision>

    <decision name="TestDate">
        <variable typeRef="date" name="TestDate"/>
        <informationRequirement>
            <requiredInput href="#_input_date"/>
        </informationRequirement>
        <literalExpression>
            <text>Birthday + @"P1Y"</text>
        </literalExpression>
    </decision>

    <decision name="TestDateTime">
        <variable typeRef="dateTime" name="TestDateTime"/>
        <informationRequirement>
            <requiredInput href="#_input_date_time"/>
        </informationRequirement>
        <literalExpression>
            <text>NextMeeting + @"P1DT2H"</text>
        </literalExpression>
    </decision>

    <decision name="TestTime">
        <variable typeRef="time" name="TestTime"/>
        <informationRequirement>
            <requiredInput href="#_input_time"/>
        </informationRequirement>
        <literalExpression>
            <text>WakeUp + @"-PT2H"</text>
        </literalExpression>
    </decision>

    <decision name="TestYearMonthDuration">
        <variable typeRef="yearMonthDuration" name="TestYearMonthDuration"/>
        <informationRequirement>
            <requiredInput href="#_input_year_month_duration"/>
        </informationRequirement>
        <literalExpression>
            <text>School + @"P1Y3M"</text>
        </literalExpression>
    </decision>

    <decision name="TestDayTimeDuration">
        <variable typeRef="dayTimeDuration" name="TestDayTimeDuration"/>
        <informationRequirement>
            <requiredInput href="#_input_day_time_duration"/>
        </informationRequirement>
        <literalExpression>
            <text>Wedding - @"P1DT3H"</text>
        </literalExpression>
    </decision>

    <decision name="TestFunction">
        <variable typeRef="Any" name="TestFunction"/>
        <literalExpression>
            <text>function(x,y) x + y</text>
        </literalExpression>
    </decision>

    <inputData name="Full Name" id="_input_string">
        <variable typeRef="string" name="Full Name"/>
    </inputData>

    <inputData name="Age" id="_input_number">
        <variable typeRef="number" name="Age"/>
    </inputData>

    <inputData name="Married" id="_input_boolean">
        <variable typeRef="boolean" name="Married"/>
    </inputData>

    <inputData name="Birthday" id="_input_date">
        <variable typeRef="date" name="Birthday"/>
    </inputData>

    <inputData name="NextMeeting" id="_input_date_time">
        <variable typeRef="dateTime" name="NextMeeting"/>
    </inputData>

    <inputData name="WakeUp" id="_input_time">
        <variable typeRef="time" name="WakeUp"/>
    </inputData>

    <inputData name="School" id="_input_year_month_duration">
        <variable typeRef="yearMonthDuration" name="School"/>
    </inputData>

    <inputData name="Wedding" id="_input_day_time_duration">
        <variable typeRef="dayTimeDuration" name="Wedding"/>
    </inputData>
</definitions>
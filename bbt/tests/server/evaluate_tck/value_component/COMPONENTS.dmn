<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<definitions namespace="https://dmntk.io/2_0001"
             name="ValueDto"
             xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/">
    <description>Test for ValueDto</description>

    <itemDefinition name="tComponent">
        <itemComponent name="pString">
            <typeRef>string</typeRef>
        </itemComponent>
    </itemDefinition>

    <decision name="TestComponent">
        <variable typeRef="tComponent" name="TestComponent"/>
        <informationRequirement>
            <requiredInput href="#_input_component"/>
        </informationRequirement>
        <literalExpression>
            <text>Component</text>
        </literalExpression>
    </decision>

    <inputData name="Component" id="_input_component">
        <variable typeRef="tComponent" name="Component"/>
    </inputData>

</definitions>